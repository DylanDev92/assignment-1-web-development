<!-- This will display the information of the portfolio projects -->
<head>
  <title><%= title %></title>
  <link rel="stylesheet" href="/stylesheets/projects.css" />
</head>
<div class="projects">
  <h1>Projects</h1>
  <p>
    These are some projects I've been working on, I'll be constantly updating
    it.
  </p>
  <p>Visit my <a href="<%= portfolio.github %>">GitHub</a> for more projects</p>
  <ul class="list-projects">
    <% portfolio.projects.forEach(project => { %>
    <li>
      <%- include('./partials/card.ejs', {info: project, src:
      "/images/project-images/"}) %>
    </li>
    <% }) %>
  </ul>
</div>
