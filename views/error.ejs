<!-- This endpoint will redirect the user to the home page. 
     This can happen if the user access to a route that doesn't exist. -->
<div class="error">
  <h1><%= error.status %>: <%= message %></h1>
  <h3>Redirecting to home...</h3>
</div>
<script>
  setTimeout(() => {
    window.location.href = "/";
  }, 1000);
</script>
