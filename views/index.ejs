<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/home.css">
    <link rel="stylesheet" href="/stylesheets/partials/tech-stack.css">
  </head>
  <section class="home-section">
    <!-- Here I include the profile -->
    <%- include('./partials/profile.ejs') %>

    <div class="right-content">
      <div class="brief-projects">
        <%- include('./partials/briefItems.ejs', { label: "Projects", link: "projects",
        items: portfolio.projects}) %>
      </div>

      <!-- Displays the tech-stack from portfolio.json -->
      <div class="tech-stack">
        <h2>Tech Stack</h2>
        <div class="stack">
          <% portfolio.tech_stack.forEach(tech => { %>
          <img
            src="/images/techstack/<%= tech %>.png"
            height="50px"
            width="50px"
          />
          <% }) %>
        </div>
      </div>
    </div>
  </section>
</html>
