<!-- This will display the information of the portfolio services -->

<head>
  <title><%= title %></title>
  <link rel="stylesheet" href="/stylesheets/services.css" />
</head>
<div class="services">
  <h1>I offer these services</h1>
  <ul class="service-list">
    <% portfolio.services.forEach(service => { %>
    <li>
      <%- include('./partials/card.ejs', {info: service, src:
      "/images/services/"}) %>
    </li>
    <% }) %>
  </ul>
</div>
